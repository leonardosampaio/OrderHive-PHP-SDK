{
  "name": "orderhive/php-sdk",
  "description": "OrderHive PHP SDK",
  "keywords": ["orderhive", "api", "sdk"],
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "ivanner",
      "email": "ivanner@gmail.com",
      "homepage": "https://github.com/ivanner"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-curl": "*",
    "guzzlehttp/guzzle": ">=6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.8"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
  },
  "autoload": {
    "psr-4": {
      "OrderHive\\": "src/OrderHive/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrderHive\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
